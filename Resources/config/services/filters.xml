<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="antwebes_chateaclient_bundle.api_filter_collection.class">Ant\Bundle\ChateaClientBundle\Api\Query\Filter\FilterCollection</parameter>
        <parameter key="antwebes_chateaclient_bundle.api_filter_channel_type">Ant\Bundle\ChateaClientBundle\Api\Query\Filter\FilterChannelType</parameter>
    </parameters>
        <services>
            <service id="antwebes_chateaclient_bundle.api_filter_channel_type" class="%antwebes_chateaclient_bundle.api_filter_channel_type%" public="false"/>
            <service id="antwebes_chateaclient_bundle.filter.channels" class="%antwebes_chateaclient_bundle.api_filter_collection.class%" public="false">
                <call method="add">
                    <argument id="antwebes_chateaclient_bundle.api_filter_channel_type" type="service" />
                </call>
            </service>
            <service id="antwebes_api_filter_channel"  alias="antwebes_chateaclient_bundle.filter.channels"  synthetic="true" />
        </services>
</container>
