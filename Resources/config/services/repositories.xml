<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="antwebes_chateaclient_bundle.api_repository_channel.class">Ant\Bundle\ChateaClientBundle\Repositories\ChannelRepository</parameter>
        <parameter key="antwebes_chateaclient_bundle.api_repository_user.class">Ant\Bundle\ChateaClientBundle\Repositories\UserRepository</parameter>
        <parameter key="antwebes_chateaclient_bundle.api_model_channel.class">Ant\Bundle\ChateaClientBundle\Api\Model\Channel</parameter>
        <parameter key="antwebes_chateaclient_bundle.api_model_user.class">Ant\Bundle\ChateaClientBundle\Api\Model\User</parameter>
    </parameters>
        <services>
            <service id="antwebes_chateaclient_bundle.api_repository_channel" class="%antwebes_chateaclient_bundle.api_repository_channel.class%" public="false">
                <argument>%antwebes_chateaclient_bundle.api_model_channel.class%</argument>
            </service>


            <service id="antwebes_chateaclient_bundle.api_repository_channel"  alias="api_channel_repository"  synthetic="true" />
        </services>
</container>
