<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">   
    
    <parameters>
    	<parameter key="antwebes_chateaclient_lib.api.class">Ant\ChateaClient\Client\Api</parameter>
    	<parameter key="antwebes_chateaclient_bundle.api.channles.class">Ant\Bundle\ChateaClientBundle\Services\Api\Channels</parameter>
    </parameters>
    <services>
          
        <service id="antwebes_chateaclient_lib.api" class="%antwebes_chateaclient_lib.api.class%" public="true">           
            <argument type="service" id="chatea_client.http_client" />
        </service>
		 
        <service id="antwebes_chateaclient_lib.api.abstract" public="false"  abstract="true" >
        	<argument type="service" id="antwebes_chateaclient_lib.api" />
        </service>
        <service id="antwebes_chateaclient_lib.api.channels" 
        		 class="%antwebes_chateaclient_bundle.api.channles.class%" 
        		 public="false" 
        		 parent="antwebes_chateaclient_lib.api.abstract" >
        		 <argument type="service" id="antwebes_chateaclient_lib.api" />
        </service>
        
        <service id="antwebes_channels" alias="antwebes_chateaclient_lib.api.channels" synthetic="true"  />
        
    </services>
</container> 